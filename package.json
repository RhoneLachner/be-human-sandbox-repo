{
  "name": "be-human-sandbox",
  "displayName": "be-human-sandbox",
  "description": "sandbox beHuman testing",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:be-human-sandbox.helloWorld",
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "be-human-sandbox.helloWorld",
        "title": "beHuman"
      }
    ],
    "keybindings": [
      {
        "command": "be-human-sandbox.helloWorld",
        "key": "ctrl+f1",
        "mac": "cmd+i",
        "when": "editorTextFocus"
    }
  ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vscode-test": "^1.4.1"
  }
}
